<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="edicsemJPASie" transaction-type="JTA">
		<provider>org.hibernate.ejb.HibernatePersistence</provider>
		<jta-data-source>java:/edicsemJPADatasource</jta-data-source>
		<class>com.edicsem.pe.sie.entity.Persona</class>
		<class>com.edicsem.pe.sie.entity.Usuario</class>
	<properties>
			<property name="eclipselink.jdbc.driver" value="com.mysql.jdbc.Driver" />
			<property name="eclipselink.jdbc.url" value="jdbc:mysql://localhost:3306/test" />
			<property name="eclipselink.jdbc.user" value="root" />
			<property name="eclipselink.jdbc.password" value="admin" />
			<property name="eclipselink.logging.level" value="FINE" />
			<property name="jboss.entity.manager.factory.jndi.name"
				value="java:/sieSoftJPAEntityManagerFactory" />
		</properties>
	</persistence-unit>
</persistence>
